
@{
    ViewBag.Title = "real";
    Layout = "~/Views/Shared/_LayoutAsk.cshtml";
}
<div class="container">
    <form id="form1" action="@Url.Action("real_ask")@HttpUtility.UrlDecode("?id=0")" method="post">
        <div class="row">
            <h1>答题开始</h1>
            <h4>欢迎 @ViewData["username"] 请开始今天第:@ViewData["qdseq"]次答题</h4>

            <div class="col-md-3">
                <button type="submit" class="btn btn-info col-md-6 col-xs-6">开始答题</button>
            </div>

        </div>
    </form>
    </div>
